<div class="col-lg-8 box-white mt-5 mb-5">
    <select class="form-control form-image" data-live-search="true" id="changeStatistic">
        <% categoryData.forEach(row => { %>
            <option value="<%= row._id %>"><%= row.nama %></option>
        <% }) %>
    </select>
    <div class="container mt-4">
        <div class="row">
            <div class="col">
                <div class="chart">
                    <canvas id="statistic" width="1200"></canvas>
                    <div id="noData"><p class="text-center text-white">Tidak ada data</p></div>
                </div>
            </div>
        </div>
        <div class="row mt-3">
            <h5 class="text-brown text-uppercase">Riwayat Try Out</h5>
        </div>
        <div class="row text-center mt-3 mb-3" id="riwayatStatistic">
        </div>
    </div>
</div>