<div class="col-lg-6 mt-5 mb-5">
    <div class="p-2 col-title mx-auto">
        <h5 class="text-brown text-center">PERINGKAT</h5>
    </div>
    <div class="container box-white mt-5 text-center">
        <div class="row m-5">
            <div class="col-lg-12">
                <table class="table table-hover table-borderless" id="tablePeringkat" width="100%">
                    <thead>
                        <tr>
                            <th>Urutan</th>
                            <th>Nama</th>
                            <th>Score</th>
                            <th>-</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% scoreData.forEach(data => { %>
                            <tr class="w-100">
                                <td class="text-white"><%= data.no %></td>
                                <td class="w-100">
                                    <span class="text-white"><%= data.namalengkap %></span><br>
                                    <!-- <span class="text-warning">TO 12345</span> -->
                                </td>
                                <td class="text-white">
                                    <span class="text-warning">Skor</span>
                                </td>
                                <td><p class="col-circle text-white text-bold"><%= data.score %></p></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>