<div class="col-lg-8 mt-5 mb-5">
    <div class="p-2 col-title mx-auto">
        <h5 class="text-brown text-center">TRY OUT</h5>
    </div>
    <div class="p-2 mt-5">
        <select class="form-control" data-live-search="true" id="changeTryout">
            <% if (tryoutData.length != undefined) { %> 
                <% tryoutData.forEach(row => { %>
                    <option value="<%= row._id %>"><%= row.nama %></option>
                <% }) %>
            <% } else { %>
                <option value="<%= tryoutData._id %>"><%= tryoutData.nama %></option>
            <% } %>
        </select>
    </div>  
    <div class="container mt-5" id="listSoalTryout">

    </div>
</div>

<%- include('../../modals/tryout/konfirmasi') %>